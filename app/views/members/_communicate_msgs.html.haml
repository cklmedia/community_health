%ul
  %span#m_get_msg_time{:style => "display:none"}= Time.now
  - DoctorCommunicateMember.get_two_communicate(current_user.doctor_id,current_user.id).each do |c|
    %li{:class => member_sending_float(c.sending)}
      %i
        = c.msg
        %span="#{c.created_at.strftime("%Y-%m-%d %H:%M:%S")}"